{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "commonjs",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "upload-exif-plugin": ["./src/"],
      "upload-exif-plugin/*": ["./src/*"]
    }
  },
  "include": ["./**/*"],
  "exclude": ["./node_modules/**/*"]
}
